#include <iostream>
using namespace std;

void verifyUsername();
int main()
{
    verifyUsername();

    return 0;
}

void verifyUsername()
{
    string username = "CodeMaster123";
    string input;

    while (input != username)
    {
        cout << "What is your username?" << endl;
        cin >> input;
    }
    cout << "Access Granted" << endl;
}
